{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-undef": 0,
        "no-underscore-dangle": "off",
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-prototype-builtins": 0,
        "consistent-return":0,
        "import/prefer-default-export":0,
        "no-extra-semi": 0,
        "prefer-destructuring": 0
    }
}
